install.packages("magrittr")
install.packages("dplyr")
install.packages("stringr")
library(magrittr) 
library(dplyr)
library(stringr)
data(package = .packages(all.available = TRUE))
bi <- data.frame(band_instruments)
bi[2,2] <- 'bass player bassion'
bi <- bi %>%
mutate(
bi2 = ifelse(grepl('(bass)', plays), 1, 0)
)
bi <- bi %>%
  mutate(
    bi3 = ifelse(bi2==1, str_extract(plays, "(bass)"), 0)
  )
bi <- bi[,-3]
